local EmotesName = {}
for n, _ in pairs(EmotesApi) do
  table.insert(EmotesName,n)
end

EmotesTab:CreateDropdown({
    Name = "Seleccionar emote",
    Options = EmotesName,
    Callback = function(option)
      local id = EmotesApi[option]
      Notify("Emotes","Seleccionaste el emote: "..option,1)
      PlayAnimationWithId(id)
    end
})

local Stoped = false
local SpeedValue = 1

EmotesTab:CreateSlider({
    Name = "Emote Speed",
    Range = {0, 5},
    Increment = 0.1,
    Suffix = "Speed/s",
    CurrentValue = 1,
    Callback = function(value)
        if currentTrack then
          currentTrack:AdjustSpeed(value)
          Notify("Emotes","Emote speed establecida en: "..value)
          SpeedValue = value
        end
    end
})

EmotesTab:CreateToggle({
    Name = "Pause Emote",
    CurrentValue = false, --- Valor inicial
    Callback = function(state)
        if state then
            if currentTrack then
              currentTrack:Stop()
              Stoped = true
              Name = "Play Emote"
            end
        else
            if currentTrack then
              Stoped = false
              currentTrack:Play()
              Name = "Pause Emote"
            end
        end
    end
})


RunService.Heartbeat:Connect(function()
  if not BangStatus and not FaceBangStatus and not GetBangStatus and not GetBangTwoStatus and not SuckStatus then
    if currentTrack then
      if Stoped then
        currentTrack:Stop()
      end
      currentTrack:AdjustSpeed(SpeedValue)
    end
end
end)