local Window = Library:CreateWindow({
    Name = "Admin Panel",
    Icon = 90692784970350,
    Theme = "neon_red",
    Intro = true,
    AnimationIntro = 0,
    DragImage = 127271155083726
})


local PrincipalTab = Window:CreateTab("Troll")
local PrincipalSection = PrincipalTab:CreateSection("Options")

local EmotesTab = Window:CreateTab("Emotes")
local EmotesSection = EmotesTab:CreateSection("Options")



--//Tab1

PrincipalTab:CreateInput({
    Name = "Nombre del Jugador Objetivo",
    PlaceholderText = "Escribe un nombre...",
    Callback = function(text)
      local SelectedPlayer = GetPlayerFromPartialName(text)
      if SelectedPlayer then
        Objetivo = SelectedPlayer
        Notify("Selector de jugadores","Jugador Seleccionado: "..SelectedPlayer.Name,3)
      else
        Objetivo = nil
        Notify("Selector de jugadores","No se a podido encontrar al jugador",3)
      end
    end
})

--- Botón que ejecuta script externo
FlingButton = PrincipalTab:CreateButton({
    Name = "Fling",
    Callback = function()
        Fling()
if Objetivo then     
  Notify("Fling","Haciendo fling a: "..Objetivo.Name,3)
end
end
})

TeleportButton = PrincipalTab:CreateButton({
    Name = "Teleport",
    Callback = function()
        Teleport()
if Objetivo then     
  Notify("Teleport","Te Teletransportaste a: "..Objetivo.Name,1.5)
end
end
})


PrincipalTab:CreateToggle({
    Name = "Bang",
    CurrentValue = false, --- Valor inicial
    Callback = function(state)
        if state then
            Bang()
        else
            BangStatus = state
        end
    end
})

PrincipalTab:CreateToggle({
    Name = "FaceBang",
    CurrentValue = false, --- Valor inicial
    Callback = function(state)
        if state then
            FaceBang()
        else
            FaceBangStatus = state
        end
    end
})

PrincipalTab:CreateToggle({
    Name = "GetBang1",
    CurrentValue = false, --- Valor inicial
    Callback = function(state)
        if state then
            GetBang()
        else
            GetBangStatus = state
        end
    end
})

PrincipalTab:CreateToggle({
    Name = "GetBang2",
    CurrentValue = false, --- Valor inicial
    Callback = function(state)
        if state then
            GetBangTwo()
        else
            GetBangTwoStatus = state
        end
    end
})

PrincipalTab:CreateToggle({
    Name = "Suck",
    CurrentValue = false, --- Valor inicial
    Callback = function(state)
        if state then
            Suck()
        else
            SuckStatus = state
        end
    end
})


